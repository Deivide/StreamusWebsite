@mixin-from card {
  border-radius: 2px;
  trait: lowElevation from 'box-shadow';
  trait: white from 'background-color';
  line-height: 24px;
  margin-top: 48px;
}

@mixin-from richMedia {
  /* Should be 16:9 */
}

@mixin-from primaryTitle {
  margin: 24px 16px 16px 16px;
  font-size: 24px;
  line-height: 32px;
}

@mixin-from actions {
  margin: 8px;
}

@mixin-from supportingText {
  margin: 16px 16px 24px 16px;
}